{
  "name": "sw6-theme-config-generator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "deploy": "ng build && npx angular-cli-ghpages --dir=dist/Sw6ThemeConfigGenerator",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
